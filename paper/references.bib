	date = {2005-04-15},
	pmid = {15564303},
	keywords = {Algorithms, Bacillus subtilis, Bacterial Proteins, Chromosome Mapping, Escherichia coli, Fuzzy Logic, Gene Expression Regulation, Bacterial, Models, Genetic, Models, Statistical, Operon, Sequence Alignment, Sequence Analysis, {DNA}, Signal Transduction},
	file = {Full Text:/Users/waldburger/Zotero/storage/5LV4MW25/Jacob et al. - 2005 - A fuzzy guided genetic algorithm for operon predic.pdf:application/pdf},
