	pmcid = {PMC186635},
	keywords = {Algorithms, Computational Biology, Conserved Sequence, Databases, Genetic, Escherichia coli, Genes, Archaeal, Genes, Bacterial, Genes, Fungal, Genome, Archaeal, Genome, Bacterial, Genome, Fungal, Operon},
	file = {Full Text:/Users/waldburger/Zotero/storage/KDFS9W7C/Zheng et al. - 2002 - Computational identification of operons in microbi.pdf:application/pdf},
